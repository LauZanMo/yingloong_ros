{
    "names": {
        "yl_msgs": {
            "cmake-args": [
                "-DCMAKE_BUILD_TYPE=Release"
            ]
        },
        "yl_slam_ros": {
            "cmake-args": [
                "-DCMAKE_BUILD_TYPE=Debug"
            ],
            "dependencies": [
                "absl",
                "sophus"
            ]
        },
        "absl": {
            "cmake-args": [
                "-DCMAKE_BUILD_TYPE=Release",
                "-DCMAKE_CXX_FLAGS=-fPIC",
                "-DABSL_PROPAGATE_CXX_STD=ON"
            ]
        },
        "sophus": {
            "cmake-args": [
                "-DCMAKE_BUILD_TYPE=Release",
                "-DSOPHUS_USE_BASIC_LOGGING=ON",
                "-DBUILD_SOPHUS_TESTS=OFF",
                "-DBUILD_SOPHUS_EXAMPLES=OFF"
            ]
        }
    }
}